{
  "private": true,
  "name": "redux-study",
  "version": "0.0.1",
  "description": "redux study",
  "scripts": {
    "eslint": "eslint src/{*,**/*}.{es6,jsx}",
    "browserify": "browserify --verbose --debug --extension=.jsx --extension=.es6 -t [ babelify ] src/index.es6 | uglifyjs -o dist/index.js",
    "watchify": "watchify --verbose --debug --extension=.jsx --extension=.es6 -t [ babelify ] src/index.es6 -o dist/index.js",
    "watch": "parallelshell 'chokidar \"src/**.{es6,jsx}\" -c \"npm run eslint\"' 'npm run watchify'",
    "clean": "rm -rf dist/"
  },
  "keywords": [
    "redux"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "reactify": "^1.1.1",
    "redux": "^3.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-register": "^6.7.2",
    "chokidar-cli": "^1.2.0",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^4.3.0",
    "parallelshell": "^2.0.0",
    "watchify": "^3.7.0"
  }
}
